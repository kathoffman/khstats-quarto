	keywords = {Anti-Bacterial Agents, Child, CHILD HEALTH, Cohort Studies, Diarrhea, DIARRHOEA, DRUG MISUSE, Epidemiological methods, Humans, Incidence, India, Infant, PUBLIC HEALTH},
	pages = {500--505},
